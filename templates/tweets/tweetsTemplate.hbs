<div class="tweets">
	<p>Results from search:</p>

	{{#if tweets}}
		{{#each tweets}}
	        TEXT OF TWEET:
	        {{text}}
	        USER:
	        <a href="#/map/{{from_user}}">
	        {{from_user}}</a>
	        <br/>
	    {{/each}}
    {{else}}
    	no tweets found matching search criteria
	{{/if}}
</div>